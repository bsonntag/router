    "@babel/preset-react": "^7.6.3",
    "@bsonntag/eslint-config": "^0.5.0",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "parcel": "^1.12.4",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
